{
  "name": "redux-reuse",
  "version": "0.2.2",
  "description": "Reuse code in redux reducers",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run build:commonjs; npm run build:es6; npm run build:umd",
    "build:commonjs": "babel src --out-dir lib",
    "build:es6": "NODE_ENV=es6 babel src --out-dir es6",
    "build:umd": "webpack --progress; NODE_ENV=production webpack -p --progress",
    "test": "NODE_ENV=test karma start --single-run",
    "tdd": "NODE_ENV=test karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyberInt/redux-reuse.git"
  },
  "authors": [
    "Max Foo <maxf@cyberint.com> (https://github.com/xamfoo)",
    "Konstantin Belenkiy <belkosyak@gmail.com> (https://github.com/belkosyak)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CyberInt/redux-reuse/issues"
  },
  "homepage": "https://github.com/CyberInt/redux-reuse#readme",
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-es2015-webpack": "6.4.1",
    "babel-preset-stage-1": "6.5.0",
    "babel-preset-stage-2": "6.11.0",
    "babel-preset-stage-3": "6.11.0",
    "babel-register": "6.9.0",
    "jasmine-core": "2.4.1",
    "karma": "1.1.0",
    "karma-clear-screen-reporter": "1.0.0",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "2.0.4",
    "karma-phantomjs-launcher": "1.0.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "redux-create-reducer": "1.1.1",
    "webpack": "1.13.1"
  },
  "dependencies": {
    "lodash": "4.13.1"
  }
}
