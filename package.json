{
  "name": "redux-reuse",
  "version": "0.1.0",
  "description": "Reuse redux reducers",
  "main": "lib/reduxReuse.js",
  "scripts": {
    "build": "npm run build:commonjs; npm run build:es6; npm run build:umd",
    "build:commonjs": "babel src --out-dir lib",
    "build:es6": "NODE_ENV=es6 babel src --out-dir es6",
    "build:umd": "webpack --progress; NODE_ENV=production webpack -p --progress",
    "test": "NODE_ENV=test karma start --single-run",
    "tdd": "NODE_ENV=test karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyberInt/redux-reuse.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CyberInt/redux-reuse/issues"
  },
  "homepage": "https://github.com/CyberInt/redux-reuse#readme",
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.9.1",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-es2015-webpack": "6.4.1",
    "babel-preset-stage-1": "6.5.0",
    "babel-preset-stage-2": "6.5.0",
    "babel-preset-stage-3": "6.5.0",
    "karma": "0.13.22",
    "webpack": "1.13.1"
  }
}
